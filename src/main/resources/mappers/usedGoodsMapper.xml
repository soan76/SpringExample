<?xml version="1.0" encoding="UTF-8"?>

<!-- dtd : xml에서 사용될 태그를 정의할 문서 -->
<!-- mybatis dtd -->
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.yeom.spring.ex.database.repository.UsedGoodsRepository">
	
	<select id="selectUsedGoodsList" resultType="com.yeom.spring.ex.database.domain.UsedGoods">
		SELECT 
			`id`
			, `sellerId`
			, `title`
			, `price`
			, `description`
			, `image`
			, `createdAt`
			, `updatedAt`	
		FROM 
			`used_goods`
	</select>
	
	
	
</mapper> 